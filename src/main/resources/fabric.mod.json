{
  "schemaVersion": 1,
  "id": "tweaker",
  "version": "${version}",

  "name": "Tweaker",
  "description": "Lets you tweak various behaviour for Better Than Wolves.",
  "authors": [
    "IssaMe"
  ],
  "contact": {
    "homepage": "https://github.com/BTW-Community/HCSpawnConfig",
    "sources": "https://github.com/BTW-Community/HCSpawnConfig"
  },

  "license": "CC0-1.0",
  "icon": "assets/tweaker/icon.png",

  "environment": "*",
  "entrypoints": {
    "init": [
      "issame.tweaker.TweakerMod"
    ],
    "preLaunch": [
      "issame.tweaker.PreLaunchInitializer"
    ]
  },
  "mixins": [
    "tweaker.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4"
  },
  "suggests": {
    "flamingo": "*"
  }
}
